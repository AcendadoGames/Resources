function CompareArrays (x : Array[String], y : Array[String]) : Boolean
    {
        if(x.Count != y.Count)
        {
            return false;
        }
        for (var i = 0; i < x.Count; i += 1)
        {
            if(x.All.Current != y.All.Current)
            {
                return false;    
            }
        }
        return true;
    }